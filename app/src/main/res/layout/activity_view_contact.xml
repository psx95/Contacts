<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.psx.sqllitedatabsedemo.Model.ViewContact">
    
    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="200dp"
        android:layout_height="200dp"
        app:srcCompat="@drawable/ic_user_image_with_black_background"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteY="0dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/linearLayout21"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout41"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="15dp"
        app:layout_constraintRight_toRightOf="parent">

        <ImageView
            android:id="@+id/imageView_phone_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_phone_field"
            tools:layout_editor_absoluteX="1dp"
            android:layout_marginTop="36dp"
            android:layout_marginStart="20dp"
            tools:layout_editor_absoluteY="-5dp" />


        <TextView
            android:id="@+id/text_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ems="10"
            android:paddingBottom="20dp"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="20dp"
            />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout41"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="-471dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="parent"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout31"
        app:layout_constraintVertical_bias="0.494"
        android:layout_marginRight="8dp"
        android:layout_marginLeft="15dp"
        app:layout_constraintRight_toRightOf="parent">

        <ImageView
            android:id="@+id/imageView_name_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="36dp"
            app:srcCompat="@drawable/ic_add_contact_field"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="-5dp" />


        <TextView
            android:id="@+id/text_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="30dp"
            android:layout_weight="1"
            android:ems="10"
            android:paddingBottom="20dp" />

    </LinearLayout>


    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/linearLayout31"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.705"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout41"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent">

        <ImageView
            android:id="@+id/imageView_email_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_mail_field"
            tools:layout_editor_absoluteX="1dp"
            android:layout_marginTop="36dp"
            android:layout_marginStart="20dp"
            tools:layout_editor_absoluteY="-5dp" />


        <TextView
            android:id="@+id/text_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ems="10"
            android:paddingBottom="20dp"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="20dp"
            />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
